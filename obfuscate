-- DirectObfuscator.Client.lua
local O = {}

function O.Execute(code)
    -- Encode
    local encoded = ""
    for i = 1, #code do
        encoded = encoded .. string.char(string.byte(code, i) ~ 99)
    end
    
    -- Decode dan execute
    local decoded = ""
    for i = 1, #encoded do
        decoded = decoded .. string.char(string.byte(encoded, i) ~ 99)
    end
    
    -- Gunakan load atau loadstring yang tersedia
    local loader = loadstring or load
    local func = loader(decoded)
    if func then
        func()
    end
end

return O
