-- GitHubObfuscator.lua (Simpan di GitHub)
local O = {}

function O.Execute(code)
    -- Simple XOR encoding
    local encoded = ""
    for i = 1, #code do
        local byte = string.byte(code, i)
        encoded = encoded .. string.char(byte ~ 77)
    end
    
    -- Decode and execute
    local decoded = ""
    for i = 1, #encoded do
        decoded = decoded .. string.char(string.byte(encoded, i) ~ 77)
    end
    
    -- Universal loader
    local loader = loadstring or load
    if loader then
        local func = loader(decoded)
        if func then
            return func()
        end
    end
    return "Execution failed"
end

return O
