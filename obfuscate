-- NoDepObfuscator.Client.lua
_G.OBFUSCATOR = {}

function _G.OBFUSCATOR.Run(code)
    local decoded = ""
    for i = 1, #code do
        decoded = decoded .. string.char(string.byte(code, i) ~ 123)
    end
    local func = (loadstring or load)(decoded)
    if func then func() end
end

return _G.OBFUSCATOR
