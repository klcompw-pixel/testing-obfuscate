-- GitHubObfuscator.lua
local O = {}

function O.Run(code)
    -- Encode code menjadi byte array
    local bytes = {}
    for i = 1, #code do
        bytes[i] = string.byte(code, i)
    end
    
    -- Build execution string langsung
    local execString = ""
    for i, byte in ipairs(bytes) do
        execString = execString .. string.char(byte)
    end
    
    -- Execute langsung menggunakan getfenv
    local func = getfenv().loadstring(execString)
    if not func then
        func = getfenv().load(execString)
    end
    
    if func then
        return func()
    else
        -- Fallback: execute langsung
        getfenv().loadstring(execString)()
    end
end

return O
